

{
  "extends": [
      "next/core-web-vitals",
      "plugin:prettier/recommended"
  ],

  "plugins": ["simple-import-sort"],

  "rules": {
     "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
      "prettier/prettier": [
          "error",
          {
              "endOfLine": "auto"
          }
      ]
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
